  "compilerOptions": {
    "target": "es5",                       
    "module": "commonjs",                  
    "strict": true,                           
    "esModuleInterop": true,   
    "resolveJsonModule": true,
    "isolatedModules": true,              
    "skipLibCheck": true,                     
    "forceConsistentCasingInFileNames": false,
  }
}